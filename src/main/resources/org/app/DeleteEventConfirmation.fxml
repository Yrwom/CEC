<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.app.DeleteEventConfirmation">
   <children>
      <Button fx:id="yesDelete" layoutX="134.0" layoutY="175.0" mnemonicParsing="false" onAction="#YesDelete" prefHeight="70.0" prefWidth="103.0" text="Yes" textFill="#32bc22">
         <font>
            <Font size="28.0" />
         </font>
      </Button>
      <Button fx:id="noDelete" layoutX="354.0" layoutY="175.0" mnemonicParsing="false" onAction="#NoDelete" prefHeight="70.0" prefWidth="103.0" text="No" textFill="#dd1616">
         <font>
            <Font size="28.0" />
         </font>
      </Button>
      <Label layoutX="124.0" layoutY="58.0" styleClass="header" text="Are you sure you'd like to delete this event?" textAlignment="CENTER" textFill="#100f0f" wrapText="true">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Label fx:id="deleteResponseCode" layoutX="8.0" layoutY="301.0" prefHeight="83.0" prefWidth="580.0" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
